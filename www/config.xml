<?xml version='1.0' encoding='utf-8'?>
<widget id="xu.li.cordova.weixin.example" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" 
xmlns:gap = "http://phonegap.com/ns/1.0">
    <name>CordovaPluginWeixinExample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    
    
           <!-- Customize your app and platform with the preference element. -->
    <preference name="phonegap-version"           value="3.5.0" />          <!-- all: current version of PhoneGap -->

    <gap:plugin name="xu.li.cordova.wechat" version="0.4" />

    <preference name="weixinappid" value="cordovapluginweixinexample" />
</widget>
